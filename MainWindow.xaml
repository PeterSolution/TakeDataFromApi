<Window x:Class="OperationOnData.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OperationOnData"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid x:Name="Postbtn" ContextMenuClosing="Button_Click">
        <Button x:Name="getmodels" Content="Get All Models" HorizontalAlignment="Left" Margin="21,10,0,0" VerticalAlignment="Top" Click="getmodels_Click"/>
        <ListBox x:Name="Data" d:ItemsSource="{d:SampleData ItemCount=0}" Margin="215,0,0,0"/>
        <Button x:Name="GetModelId" Content="Get Selecte Model" HorizontalAlignment="Left" Margin="21,35,0,0" VerticalAlignment="Top" Click="GetModelId_Click"/>
        <Button Content="Add new position" HorizontalAlignment="Left" Margin="22,60,0,0" VerticalAlignment="Top" Click="Button_Click"/>

    </Grid>
</Window>
